{
    "name": "linguagem-egua-vscode",
    "displayName": "Linguagem Egua",
    "description": "Extens√£o de sintaxe para o Visual Studio Code na linguagem Egua",
    "icon": "recursos/icon.png",
    "publisher": "LinguagemEgua",
    "repository": {
        "type": "git",
        "url": "https://github.com/eguatech/egua-extensao-vscode"
    },
    "version": "0.4.0",
