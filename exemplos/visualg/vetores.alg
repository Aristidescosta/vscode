algoritmo "Ex-038"
var
    num1, num2, num3, soma, item: inteiro
    listaTeste: vetor [1..5] de inteiro
    contador: inteiro
inicio
    para contador de 1 ate 5 faca
        escreva("Digite uma nota: ")

        leia(listaTeste[contador])

        // leia(item)
        // listaTeste[contador] <- item
    fimpara

    escreval(listaTeste[1])

fimalgoritmo